#
# Makefile to compile c programs for CSCI230  homework 11
#
# Blake Sorenson - 170413: Written
#

CC=g++
CFLAGS=-c -Wall

# Create executable files
all: test_code.o 
	$(CC) -Wall -o test_exec test_code.o 

# Create hw11-bsorenson.o object file by specifying source c code
test_code.o: test_code.cpp
	$(CC) $(CFLAGS) test_code.cpp

# Remove all object files generated by this makefile
clean:
	rm *.o test_exec
